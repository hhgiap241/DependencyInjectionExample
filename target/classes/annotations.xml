<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <context:annotation-config/>
    <bean id="student" class="com.kms.giaphoang.overview.annotations.Student">
        <property name="name" value="Giaphoang"/>
        <property name="age" value="20"/>
    </bean>
    <bean id="student2" class="com.kms.giaphoang.overview.annotations.Student"/>
    <bean id="studentId" class="com.kms.giaphoang.overview.annotations.StudentIdCard">
        <constructor-arg value="19127640"/>
    </bean>
    <bean id="profile" class="com.kms.giaphoang.overview.annotations.Profile">
    </bean>
</beans>